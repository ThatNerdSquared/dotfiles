{
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.enablePreview": false,
    "editor.tabCompletion": "on",
    "editor.renderControlCharacters": false,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDragAndDrop": false,
    "editor.snippetSuggestions": "top",
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Rosé Pine Dawn",
    "workbench.preferredDarkColorTheme": "Radical",
    "prettier.useTabs": true,
    "editor.acceptSuggestionOnEnter": "off",
    "markdown.extension.list.indentationSize": "inherit",
    "files.associations": {
        "*.ino": "cpp"
    },
    "workbench.editorAssociations": {
        "*.pdf": "pdf.preview",
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "quill-icons",
    "C_Cpp.default.compilerPath": "/usr/bin/clang",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "todo-tree.tree.showScanModeButton": false,
    "keyboard.dispatch": "keyCode",
    "markdown-links.graphType": "obsidian",
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "latex"
    ],
    "vim.cursorStylePerMode.insert": "line-thin",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
          "before": ["k"],
          "after": ["g", "k"]
        },
        {
          "before": ["j"],
          "after": ["g", "j"]
        }
    ],
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "foam.files.ignore": [
        "**/index.md"
    ],
    "files.exclude": {
        "**/index.md": true
    },
    "files.watcherExclude": {
        "**/index.md": true
    },
    "editor.accessibilitySupport": "off",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "relative",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "security.workspace.trust.banner": "never",
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "workbench.editor.splitInGroupLayout": "vertical",
    "gitlens.defaultDateFormat": "YYYY-MM-DD",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "gitlens.defaultTimeFormat": "hh:mm",
    // "powermode.enabled": true,
    // "powermode.presets": "flames"
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vscode-neovim.neovimExecutablePaths.win32": " C:\\tools\\neovim\\Neovim\\bin\\nvim.exe",
    "svelte.enable-ts-plugin": true,
    "debug.onTaskErrors": "showErrors",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontFamily": "iA Writer Quattro, Menlo, Monaco, 'Courier New', monospace",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "criticmarkup.addition",
                "settings": {
                    "foreground": "#00bb00"
                }
            },
            {
                "scope": "criticmarkup.deletion",
                "settings": {
                    "foreground": "#dd0000"
                }
            },
            {
                "scope": "criticmarkup.substitution",
                "settings": {
                    "foreground": "#ff8600"
                }
            },
            {
                "scope": "criticmarkup.comment",
                "settings": {
                    "foreground": "#0000bb"
                }
            },
            {
                "scope": "criticmarkup.highlight",
                "settings": {
                    "foreground": "#aa53a9"
                }
            }
        ]
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "arduino.useArduinoCli": true,
    "python.formatting.provider": "black",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "zenMode.fullScreen": false,
    "dart.debugExternalPackageLibraries": false,
    "dart.debugSdkLibraries": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Rosé Pine Dawn",
    "workbench.activityBar.location": "top",
    "window.commandCenter": false,
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.editor.showTabs": "single",
}