#.load "yabai-bindings-skhdrc"

# safety on for widgets
shift + ctrl - s: osascript -e "tell application id \"tracesOf.Uebersicht\" to set hidden of widget id \"Nerdsmirk-jsx\" to true"

# safety off for widgets
shift + ctrl + alt - s: osascript -e "tell application id \"tracesOf.Uebersicht\" to set hidden of widget id \"Nerdsmirk-jsx\" to false"

# sticky/unsticky the window
shift + ctrl + cmd - s: yabai -m window --toggle sticky

# spacy clipboard
shift + alt + cmd - c: python3 ~/dotfiles/scripts/spacy.py

# TOUCHBAR TO KEYPAD
alt - f13: python3 ~/dotfiles/scripts/touchbar-toggl.py
alt - f14: python3 ~/dotfiles/scripts/touchbar-codespaces.py
alt - f15: python3 ~/dotfiles/scripts/nukes.py
alt - f16: /usr/bin/automator /Users/nathanyeung/Library/Services/NerdPods.workflow
