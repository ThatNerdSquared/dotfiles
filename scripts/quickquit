#!/usr/bin/env python3

# Required parameters:
# @raycast.schemaVersion 1
# @raycast.title QuickQuit
# @raycast.mode silent

# Optional parameters:
# @raycast.icon ðŸ§¨

# Documentation:
# @raycast.author ThatNerdSquared
# @raycast.authorURL https://github.com/ThatNerdSquared

import subprocess

TO_QUIT = [
    'mpv',
    'IINA',
    'Discord',
    'Slack',
    'SUPERHOT',
    'GRIS',
    'Stardew Valley',
    'Into the Breach',
    'Hollow Knight',
    'The Battle for Wesnoth',
    'Steam',
]

for app in TO_QUIT:
    subprocess.run(f'osascript -e \'quit app \"{app}\"\'')

print('Nuke launched successfully')
